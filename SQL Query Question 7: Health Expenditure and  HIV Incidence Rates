This 1st query is measuring current health expenditure (% of GDP) in 2005-2013 for 12 select countries
SELECT country_code, year, indicator_name, 
ROUND(AVG(value), 2) AS average_value,
FROM `bigquery-public-data.world_bank_wdi.indicators_data` 
WHERE country_code IN ('USA','CAN','ZAF','NGA','FRA','AFG','COL','COG','GHA','IND','MLI','DOM')
AND indicator_name = 'Current health expenditure (% of GDP)' 
AND year BETWEEN 2005 and 2013
GROUP BY country_code, year, indicator_name 

This 2nd query is for measuring current health expenditure (% of GDP) in 2014-2020 for 12 select countries
SELECT country_code, year, indicator_name, 
ROUND(AVG(value), 2) AS average_value,
FROM `bigquery-public-data.world_bank_wdi.indicators_data` 
WHERE country_code IN ('USA','CAN','ZAF','NGA','FRA','AFG','COL','COG','GHA','IND','MLI','DOM')
AND indicator_name = 'Current health expenditure (% of GDP)' 
AND year BETWEEN 2014 and 2022
GROUP BY country_code, year, indicator_name

This 3rd query is for HIV Incidence Rate above 50 in 2005-2013 for 12 select countries.
SELECT indicator_name AS measuring_factor,
 ROUND(value, 2) AS measuring_value,
 country_code, 
 country_name,
 year,
 FROM `bigquery-public-data.world_bank_health_population.health_nutrition_population` 
WHERE indicator_name LIKE '%Incidence of HIV, ages 50+ (per 1,000 uninfected population ages 50+)%'
AND year BETWEEN 2005 and 2013
AND country_code IN ('AFG','MLI','COG','GHA','NGA','DOM','IND','COL','ZAF','CAN','FRA','USA')

This 4th query is for HIV Incidence Rate above 50 in 2014-2020 for 12 select countries.
SELECT indicator_name AS measuring_factor,
 ROUND(value, 2) AS measuring_value,
 country_code, 
 country_name,
 year,
 FROM `bigquery-public-data.world_bank_health_population.health_nutrition_population` 
WHERE indicator_name LIKE '%Incidence of HIV, ages 50+ (per 1,000 uninfected population ages 50+)%'
AND year BETWEEN 2014 and 2020
AND country_code IN ('AFG','MLI','COG','GHA','NGA','DOM','IND','COL','ZAF','CAN','FRA','USA') 

This 5th query is for HIV Incidence Rate between ages 15-49 in 2014-2020 for 12 select countries
SELECT indicator_name AS measuring_factor,
 ROUND(AVG(value), 2) AS average_value,
 country_code, 
 country_name,
 year,
 FROM `bigquery-public-data.world_bank_health_population.health_nutrition_population` 
WHERE indicator_name LIKE '%Incidence of HIV, ages 15-49 (per 1,000 uninfected population ages 15-49)%'
AND year BETWEEN 2014 and 2020
AND country_code IN ('AFG','MLI','COG','GHA','NGA','DOM','IND','COL','ZAF','CAN','FRA','USA')
GROUP BY indicator_name, year, country_code, country_name

This 6th query is for HIV Incidence Rate between ages 15-49 in 2005-2013 for 12 select countries.
SELECT indicator_name AS measuring_factor,
 ROUND(AVG(value), 2) AS average_value,
 country_code, 
 country_name,
 year,
 FROM `bigquery-public-data.world_bank_health_population.health_nutrition_population` 
WHERE indicator_name LIKE '%Incidence of HIV, ages 15-49 (per 1,000 uninfected population ages 15-49)%'
AND year BETWEEN 2005 and 2013
AND country_code IN ('AFG','MLI','COG','GHA','NGA','DOM','IND','COL','ZAF','CAN','FRA','USA')
GROUP BY indicator_name, year, country_code, country_name

This 7th query is for HIV Incidence Rate for all people in 2005-2013 for 12 select countries. 
SELECT indicator_name AS measuring_factor,
ROUND(value,2) AS measuring_value,
country_code,
country_name,
year,
 FROM `bigquery-public-data.world_bank_health_population.health_nutrition_population` 
WHERE indicator_name LIKE '%Incidence of HIV, all (per 1,000 uninfected population)%'
AND year BETWEEN 2005 and 2013
AND country_code IN ('USA','FRA','CAN','ZAF','COL','IND','DOM','NGA', 'GHA', 'COG', 'MLI', 'AFG')

This 8th query is for HIV Incidence Rate for all people in 2014-2020 for 12 select countries.
SELECT indicator_name AS measuring_factor,
ROUND(value,2) AS measuring_value,
country_code,
country_name,
year,
 FROM `bigquery-public-data.world_bank_health_population.health_nutrition_population` 
WHERE indicator_name LIKE '%Incidence of HIV, all (per 1,000 uninfected population)%'
AND year BETWEEN 2014 and 2020
AND country_code IN ('USA','FRA','CAN','ZAF','COL','IND','DOM','NGA', 'GHA', 'COG', 'MLI', 'AFG')
