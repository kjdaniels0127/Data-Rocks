Select HIV Incidence rates for people above 50 years old for 12 select countries (excluding Canada and France) between 2005 and 2012.
SELECT indicator_name AS measuring_factor,
country_name,
country_code,
ROUND(value,2) AS measuring_value
 FROM `bigquery-public-data.world_bank_health_population.health_nutrition_population` 
WHERE indicator_name LIKE '%Incidence of HIV, ages 50+ (per 1,000 uninfected population ages 50+)%'
AND year BETWEEN 2005 and 2012
AND country_code IN (
'USA','FRA','CAN','ZAF','COL','IND',
'DOM','NGA', 'GHA', 'COG', 'MLI', 'AFG'
)
